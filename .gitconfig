[user]
name = david üêà
email = david@fxaa.dev

[core]
editor = nvim
excludesfile = ~/.config/gitignore

[init]
defaultBranch = main

[commit]
verbose = true

[merge]
tool = vimdiff
conflictstyle = diff3

[color "status"]
	branch = normal
	added = 10 normal
	updated = 10 normal
	localbranch = normal
	changed = 11 normal
	header = normal
	remotebranch = normal
	nobranch = 1 normal italic
	unmerged = 9 normal
	untracked = normal dim
[color "branch"]
	plain = normal dim
	remote = normal dim
	current = normal
	local = normal dim
	upstream = normal
[color "remote"]
	hint = normal dim
	success = 2 normal
	error = 1 normal
	warning = 3 normal
[color "interactive"]
	header = normal bold
	prompt = 15 4
	help = normal
	error = 1 normal italic
[color "diff"]
	commit = 3 normal
	newMoved = 14 normal
	oldMovedAlternativeDimmed = 4 normal dim
	frag = normal ul
	oldMovedAlternative = 12 normal dim
	contextBold = normal bold
	newBold = 10 normal bold
	context = normal
	oldMoved = 14 normal dim
	oldBold = 9 normal dim bold
	newMovedDimmed = 6 normal
	func = normal ul
	meta = normal
	newDimmed = 2 normal
	newMovedAlternative = 12 normal
	new = 10 normal
	newMovedAlternativeDimmed = 4 normal
	whitespace = normal 9
	oldMovedDimmed = 6 normal dim
	oldDimmed = 1 normal dim
	old = 9 normal dim
[color "decorate"]
	branch = normal
	tag = 3 normal
	remotebranch = normal
	stash = normal
	grafted = 1 normal italic
	HEAD = normal dim
[diff]
	colorMoved = zebra
